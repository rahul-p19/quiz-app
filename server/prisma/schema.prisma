generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Question {
  questionId Int    @id @default(autoincrement())
  statement  String
  optiona    String
  optionb    String
  optionc    String
  optiond    String
  marks      Int
}